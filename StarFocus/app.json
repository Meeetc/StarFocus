{
  "expo": {
    "name": "StarFocus",
    "slug": "StarFocus",
    "owner": "meeet_c",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#0A0E1A"
    },
